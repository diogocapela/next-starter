{
  "presets": ["next/babel"],
  "plugins": [
    "emotion",
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "#components": "./components",
          "#config": "./config",
          "#hoc": "./hoc",
          "#hooks": "./hooks",
          "#layouts": "./layouts",
          "#pages": "./pages",
          "#public": "./public",
          "#redux": "./redux",
          "#theme": "./theme",
          "#utils": "./utils"
        }
      }
    ]
  ]
}
